{
  "name": "glickose",
  "version": "1.0.0",
  "scripts": {
    "build": "./node_modules/.bin/bnr clean && ./node_modules/.bin/bnr build",
    "clean": "./node_modules/.bin/bnr clean",
    "mrproper": "./node_modules/.bin/bnr mrproper",
    "start": "./node_modules/.bin/bnr start",
    "test": "./node_modules/.bin/bnr test"
  },
  "betterScripts": {
    "build": {
      "command": "./node_modules/.bin/webpack --colors --config webpack.config.production.js --progress",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "clean": "./node_modules/.bin/rimraf dist",
    "mrproper": "./node_modules/.bin/rimraf dist node_modules",
    "start": {
      "command": "node dev-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": "./node_module/.bin/jest"
  },
  "dependencies": {
    "bootstrap-sass": "3.3.7",
    "history": "4.6.1",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "react": "15.4.2",
    "react-bootstrap": "0.30.8",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "4.0.0",
    "react-router-dom": "next",
    "react-router-redux": "next",
    "redux": "3.6.0",
    "redux-devtools-extension": "2.13.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "better-npm-run": "0.0.15",
    "css-loader": "0.28.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "19.0.2",
    "node-sass": "4.5.2",
    "postcss-loader": "1.3.3",
    "react-hot-loader": "next",
    "react-test-renderer": "15.4.2",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.1",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2"
  }
}
